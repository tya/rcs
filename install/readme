#install vim instructions
sudo apt-get install libgnome2-dev libgtk2.0-dev libatk1.0-dev libbonoboui2-dev libcairo2-dev libx11-dev libxpm-dev libxt-dev libperl-dev

sudo apt-get install python-dev libncurses5-dev ruby ruby-dev

./configure --enable-perlinterp --enable-pythoninterp --enable-rubyinterp --enable-cscope  --with-features=huge --prefix=$directory

slate alfred
cVim chrome
homebrew install coreutils

xbindkeys wmctrl zenity
xbindkeys -f keys.rc
