# vim: set ft=sh:
export PH=$HOME/projects/
export PT=$HOME/projects/todo
export DL=$HOME/downloads
export SB=$HOME/sandbox
export CH=/home/share/configs

alias work='cd $PH'
alias dl='cd $DL'
alias play='cd $SB'
alias dual='$CH/xstuff/dual.sh'
alias cfg='cd $CH'
alias tmuxdev='$CH/configs/dev-tmux.sh'
alias tmuxhome='$CH/configs/home-tmux.sh'
